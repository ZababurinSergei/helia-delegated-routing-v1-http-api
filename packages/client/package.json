{
  "name": "@helia/delegated-routing-v1-http-api-client",
  "version": "3.0.1",
  "description": "A Delegated Routing V1 HTTP API client",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipfs/helia-delegated-routing-v1-http-api/tree/master/packages/client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/helia-delegated-routing-v1-http-api.git"
  },
  "bugs": {
    "url": "https://github.com/ipfs/helia-delegated-routing-v1-http-api/issues"
  },
  "keywords": [
    "IPFS"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "build": "aegir build",
    "build:esbuild": "nodemon --config esbuild.watch.json esbuild.mjs",
    "test": "aegir test",
    "test:node": "aegir test -t node --cov"
  },
  "dependencies": {
    "@libp2p/interface": "^1.6.3",
    "@libp2p/logger": "^4.0.19",
    "@libp2p/peer-id": "^4.2.3",
    "@multiformats/multiaddr": "^12.3.0",
    "any-signal": "^4.1.1",
    "browser-readablestream-to-it": "^2.0.7",
    "ipns": "^9.1.0",
    "it-first": "^3.0.6",
    "it-map": "^3.1.1",
    "it-ndjson": "^1.0.7",
    "multiformats": "^13.2.2",
    "p-defer": "^4.0.1",
    "p-queue": "^8.0.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@libp2p/peer-id-factory": "^4.2.3",
    "aegir": "^44.1.1",
    "body-parser": "^1.20.2",
    "esbuild": "^0.20.2",
    "esbuild-css-modules-plugin": "^3.1.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-glsl": "^1.2.2",
    "esbuild-plugin-path-alias": "^1.0.7",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "it-all": "^3.0.6"
  },
  "sideEffects": false
}
